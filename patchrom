#!/bin/bash
SAUCE=~/android/cm-11.0

# Let's apply all commits!
echo "----------------------------------------"
echo "----------------------------------------"

repopick -f -t cm11.0-appops-su
repopick -t bootzip-cm-11.0
repopick -t asb-2019.02-cm11
repopick -t asb-2019.03-cm11
repopick -t asb-2019.04-cm11
repopick -t asb-2019.05-cm11
repopick -t asb-2019.06-cm11
repopick -t asb-2019.07-cm11
repopick -t asb-2019.08-cm11
repopick -t asb-2019.09-cm11
repopick -t asb-2019.10-cm11
repopick -t asb-2019.11-cm11
repopick -t asb-2019.12-cm11
repopick -t asb-2020.01-cm11

echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/packages/apps/Settings
git fetch https://github.com/LineageOS/android_packages_apps_Settings refs/changes/54/230054/8 && git cherry-pick FETCH_HEAD
git fetch https://github.com/LineageOS/android_packages_apps_Settings refs/changes/92/230392/12 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"

cd $SAUCE/pull
. patch.sh
