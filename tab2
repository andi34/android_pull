echo "----------------------------------------"
echo "fix compiling on KitKat"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_hardware_ti_omap4 refs/changes/00/126500/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
