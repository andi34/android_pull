#!/bin/bash
# Your build source code directory path. In the example below the build source code directory path is in the "home" folder. If your source code directory is on an external HDD it should look like: //media/your PC username/the name of your storage device/path/to/your/source/code/folder
SAUCE=~/android/mm6.0

# Let's apply all commits!
echo ""
echo "----------------------------------------"
cd $SAUCE/device/samsung/espressowifi
echo "----------------------------------------"
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espressowifi refs/changes/04/21304/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/espresso3g
echo "----------------------------------------"
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/81/20181/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/82/20182/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/83/20183/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/84/20184/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/85/20185/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/86/20186/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/87/20187/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/88/20188/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/89/20189/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/90/20190/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/91/20191/2 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/device_samsung_espresso3g refs/changes/92/20192/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/kernel/samsung/espresso10
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/hardware/ti/omap4
echo "----------------------------------------"
git pull ssh://andi34@review.slimroms.org:29418/SlimRoms/hardware_ti_omap4 refs/changes/02/21302/2
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/av
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/base
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/native
echo "----------------------------------------"
git fetch https://review.slimroms.org/SlimRoms/frameworks_native refs/changes/03/21303/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/bionic
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/build
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/bootable/recovery
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/vendor/samsung
echo "----------------------------------------"
git fetch https://github.com/andi34/proprietary_vendor_samsung-2.git mm6.0-2
git cherry-pick bb19523e1e6980f23d41062ed14751539453dc05
git fetch https://github.com/andi34/proprietary_vendor_samsung-2.git mm6.0
git cherry-pick 33f17530383c27eb610756612c13bed2e5d50efe
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
cd $SAUCE
echo "----------------------------------------"
echo "----------------------------------------"
