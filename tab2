#!/bin/bash
echo ""
echo "----------------------------------------"
cd device/samsung/p3100
echo "P31XX: enable non-PIE support review.cyanogenmod.org/#/c/84785/"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_p3100 refs/changes/85/84785/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
cd device/samsung/p3110
echo "Nothing to do..."
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
cd device/samsung/p5100
echo "P51XX: enable non-PIE support review.cyanogenmod.org/#/c/84786/"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_p5100 refs/changes/86/84786/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
cd device/samsung/p5110
echo "Nothing to do..."
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo ""
cd device/samsung/omap4-common
echo "nothing to do..."
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
cd hardware/samsung
echo "----------------------------------------"
echo "Nothing for now..."
cd ../..
echo "----------------------------------------"
cd frameworks/av
echo "libstagefright: Make it possible to skip OMX buffer reallocation (2) review.cyanogenmod.org/#/c/84730/"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_frameworks_av refs/changes/30/84730/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
cd bionic
echo "Revert Revert Reenable support for non-PIE executables http://review.cyanogenmod.org/#/c/79136/"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_bionic refs/changes/36/79136/3 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ..
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
echo "----------------------------------------"
echo "----------------------------------------"
