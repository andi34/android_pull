#!/bin/bash
# Your build source code directory path. In the example below the build source code directory path is in the "home" folder. If your source code directory is on an external HDD it should look like: //media/your PC username/the name of your storage device/path/to/your/source/code/folder
SAUCE=~/android/cm-12.1

# Let's apply all commits!
echo ""
echo "----------------------------------------"
cd $SAUCE/device/samsung/p3100
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/p3110
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/p5100
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/p5110
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/espressowifi
echo "----------------------------------------"
echo "fade off by default"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_espresso refs/changes/88/137788/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "espresso: set hwrotation on init for espresso and espressowifi variants"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_espresso refs/changes/85/133985/5 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/espresso3g
echo "----------------------------------------"
echo "Rename espresso to espressowifi to avoid conflicts with HTC's espresso"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_espresso3g refs/changes/64/137664/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/espresso-common
echo "----------------------------------------"
echo "set minfree overlay for less aggressive behaviour"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_espresso-common refs/changes/02/114602/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "espresso-common: more SELinux rules "
git fetch http://review.cyanogenmod.org/CyanogenMod/android_device_samsung_espresso-common refs/changes/07/114607/3 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
cd $SAUCE/kernel/samsung/espresso10
echo "----------------------------------------"
echo "----------------------------------------"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/01/135101/1 && git cherry-pick FETCH_HEAD
# echo "BFQ"
# git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/02/110002/2 && git cherry-pick FETCH_HEAD
# echo "----------------------------------------"
# git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/03/110003/2 && git cherry-pick FETCH_HEAD
# echo "----------------------------------------"
# git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/04/110004/2 && git cherry-pick FETCH_HEAD
# echo "----------------------------------------"
# git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/05/110005/2 && git cherry-pick FETCH_HEAD
# echo "----------------------------------------"
# git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/14/110014/2 && git cherry-pick FETCH_HEAD
# echo "----------------------------------------"
#echo "enforcing time"
#git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/24/116024/1 && git cherry-pick FETCH_HEAD
# following breaks boot on p5113
#git fetch http://review.cyanogenmod.org/CyanogenMod/android_kernel_samsung_espresso10 refs/changes/41/130341/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/hardware/samsung
echo "----------------------------------------"
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/hardware/ti/omap4
echo "----------------------------------------"
echo "sgxfreq: limit frequency to default on boot"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_hardware_ti_omap4 refs/changes/00/129000/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "sgxfreq: set correct frequency limit on omap4460/4470"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_hardware_ti_omap4 refs/changes/93/137793/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "gralloc: disable GRALLOC_USAGE_CURSOR"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_hardware_ti_omap4 refs/changes/92/137792/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/av
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/base
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/vendor/cm
echo "Vendor: add hour and minute for zip-nameing"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_vendor_cm refs/changes/43/93543/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/vendor/samsung
git fetch https://github.com/andi34/proprietary_vendor_samsung-1.git cm-12.1
echo "espresso-common: revert gralloc: Recognize GRALLOC_USAGE_CURSOR as a valid usage flag."
git cherry-pick df56578ae0c87a0eb9ebd52d12a3cfbf1b593d62
echo "----------------------------------------"
echo "espresso-common: remove the unused libQmageDecoder library"
git cherry-pick 8b4bbb673af5d0cf85bfaf37306f30d93d6ff001
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/bionic
echo "Nothing for now..."
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/build
# echo "Force bsdiff for recovery_from_boot.p if not using GZIP"
# git fetch http://review.cyanogenmod.org/CyanogenMod/android_build refs/changes/90/98390/1 && git cherry-pick FETCH_HEAD
echo "add back bootzip"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_build refs/changes/78/107678/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "Set metadata on bootzip"
git fetch https://review.slimroms.org/SlimRoms/android_build refs/changes/19/7219/4 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/bootable/recovery
echo "Nothing for now..."
echo "----------------------------------------"
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
cd $SAUCE
echo "----------------------------------------"
echo "----------------------------------------"
