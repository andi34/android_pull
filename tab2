#!/bin/bash
SAUCE=~/android/android-7.1

# Let's apply all commits!
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/av
echo "----------------------------------------"
git fetch https://gerrit.omnirom.org/android_frameworks_av refs/changes/75/22775/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_av refs/changes/76/22776/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_av refs/changes/77/22777/1 && git cherry-pick FETCH_HEAD
echo ""
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/base
echo "----------------------------------------"
git fetch https://gerrit.omnirom.org/android_frameworks_base refs/changes/00/21600/5 && git cherry-pick FETCH_HEAD
echo ""
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/native
echo "----------------------------------------"
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/92/21592/3 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/93/21593/2 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/95/21595/2 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/96/21596/2 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/97/21597/2 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/98/21598/2 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/94/21594/2 && git cherry-pick FETCH_HEAD
echo ""
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
cd $SAUCE
echo "----------------------------------------"
echo "----------------------------------------"
