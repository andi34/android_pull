#!/bin/bash
SAUCE=~/android/ng7.1

# Let's apply all commits!
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/av
echo "----------------------------------------"
git fetch https://review.slimroms.org/SlimRoms/frameworks_av refs/changes/38/27238/1 && git cherry-pick FETCH_HEAD
echo ""
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/frameworks/native
echo "----------------------------------------"
git fetch https://review.slimroms.org/SlimRoms/frameworks_native refs/changes/35/27235/1 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/frameworks_native refs/changes/36/27236/1 && git cherry-pick FETCH_HEAD
git fetch https://review.slimroms.org/SlimRoms/frameworks_native refs/changes/37/27237/1 && git cherry-pick FETCH_HEAD
echo ""
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
cd $SAUCE
echo "----------------------------------------"
echo "----------------------------------------"
