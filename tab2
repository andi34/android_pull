echo "----------------------------------------"
echo "----------------------------------------"
cd frameworks/base
echo "commits for hwrotation"
git fetch https://gerrit.omnirom.org/android_frameworks_base refs/changes/32/10632/5 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "bootatnimation stuff"
