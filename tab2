echo "----------------------------------------"
echo "----------------------------------------"
cd hardware/samsung
echo "Ril fixes"
git fetch https://gerrit.omnirom.org/android_hardware_samsung refs/changes/73/11273/1 && git cherry-pick FETCH_HEAD
