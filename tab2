echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd device/samsung/p5110
echo "----------------------------------------"
echo "nothing to commit"
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd kernel/samsung/espresso10
# echo "enforcing time"
# git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/91/12091/1 && git cherry-pick FETCH_HEAD
echo "Overclocking stuff"
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/16/11216/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/17/11217/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/18/11218/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/19/11219/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/20/11220/1 && git cherry-pick FETCH_HEAD
