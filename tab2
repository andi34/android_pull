echo "----------------------------------------"
echo "----------------------------------------"
cd frameworks/native
echo "commits for hwrotation"
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/33/10633/4 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/34/10634/6 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/35/10635/6 && git cherry-pick FETCH_HEAD
