echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd device/samsung/p3110
echo "----------------------------------------"
echo "nothing to commit"
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
