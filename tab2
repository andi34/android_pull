#!/bin/bash
echo ""
echo ""
cd device/samsung/omap4-common
echo "----------------------------------------"
echo "libpower: Make it friendly to overclocked CPU https://gerrit.omnirom.org/#/c/11077/"
git fetch https://gerrit.omnirom.org/android_device_samsung_omap4-common refs/changes/77/11077/3 && git cherry-pick FETCH_HEAD
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd device/samsung/p3100
echo "P31XX: Update SELinux rules"
git fetch https://gerrit.omnirom.org/android_device_samsung_p3100 refs/changes/17/11317/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "non-PIE support"
git fetch https://gerrit.omnirom.org/android_device_samsung_p3100 refs/changes/18/11318/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "P31XX: allow OC https://gerrit.omnirom.org/#/c/11223/"
git fetch https://gerrit.omnirom.org/android_device_samsung_p3100 refs/changes/22/11222/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd device/samsung/p5100
echo "P51XX: Update SELinux rules"
git fetch https://gerrit.omnirom.org/android_device_samsung_p5100 refs/changes/19/11319/2 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "non-PIE support"
git fetch https://gerrit.omnirom.org/android_device_samsung_p5100 refs/changes/20/11320/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "P51XX: allow OC https://gerrit.omnirom.org/#/c/11223/"
git fetch https://gerrit.omnirom.org/android_device_samsung_p5100 refs/changes/23/11223/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd kernel/samsung/espresso10
echo "Overclocking stuff"
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/16/11216/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/17/11217/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/18/11218/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/19/11219/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/20/11220/1 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_kernel_samsung_espresso10 refs/changes/21/11221/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd hardware/samsung
echo "libril: move samsung specific into seperate table https://gerrit.omnirom.org/#/c/11274/"
git fetch https://gerrit.omnirom.org/android_hardware_samsung refs/changes/74/11274/3 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
cd frameworks/native
echo "Fix for landscape thumbnail images on GLES11RenderEngine https://gerrit.omnirom.org/#/c/11692/"
git fetch https://gerrit.omnirom.org/android_frameworks_native refs/changes/92/11692/1 && git cherry-pick FETCH_HEAD
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
cd frameworks/base
echo "bootatnimation stuff"
git fetch https://gerrit.omnirom.org/android_frameworks_base refs/changes/39/10639/4 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_base refs/changes/40/10640/3 && git cherry-pick FETCH_HEAD
git fetch https://gerrit.omnirom.org/android_frameworks_base refs/changes/41/10641/5 && git cherry-pick FETCH_HEAD
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
cd frameworks/av
echo "nothing to do..."
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
cd frameworks/opt/telephony
echo "nothing to do..."
cd ../../..
echo "----------------------------------------"
echo "----------------------------------------"
cd vendor/omni
echo "vendor: add date and time by default for HOMEMADE builds  https://gerrit.omnirom.org/#/c/11379/"
git fetch https://gerrit.omnirom.org/android_vendor_omni refs/changes/79/11379/6 && git cherry-pick FETCH_HEAD
cd ../..
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
echo "----------------------------------------"
echo "----------------------------------------"


