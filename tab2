#!/bin/bash
SAUCE=~/android/cm-14.1

# Let's apply all commits!
echo ""
echo "----------------------------------------"
cd $SAUCE/device/samsung/espressowifi
echo "----------------------------------------"
echo "nothing..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/espresso3g
echo "----------------------------------------"
echo "nothing..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/tuna
echo "----------------------------------------"
echo "nothing..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/device/samsung/maguro
echo "----------------------------------------"
echo "nothing..."
echo "----------------------------------------"
echo "----------------------------------------"
cd $SAUCE/hardware/ti/omap4
echo "----------------------------------------"
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/38/167638/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/39/167639/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/40/167640/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/41/167641/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/42/167642/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/43/167643/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/44/167644/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/45/167645/1 && git cherry-pick FETCH_HEAD
git fetch https://review.lineageos.org/LineageOS/android_hardware_ti_omap4 refs/changes/86/167686/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "----------------------------------------"
echo ""
echo "Done!"
cd $SAUCE
echo "----------------------------------------"
echo "----------------------------------------"
