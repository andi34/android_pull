#!/bin/bash
cd system/vold
echo "Add Flash-Friendly File System (f2fs) support. http://review.cyanogenmod.org/61238"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_system_vold refs/changes/38/61238/5 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
echo "Add exfat/ntfs support for modules or static mode. http://review.cyanogenmod.org/62307"
git fetch http://review.cyanogenmod.org/CyanogenMod/android_system_vold refs/changes/07/62307/14 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../..
echo "e2fsprogs: blkid: Add Flash-Friendly File System (f2fs) support http://review.cyanogenmod.org/61273"
cd external/e2fsprogs
git fetch http://review.cyanogenmod.org/CyanogenMod/android_external_e2fsprogs refs/changes/73/61273/1 && git cherry-pick FETCH_HEAD
echo "----------------------------------------"
cd ../..
cd build
echo "LZMA compressed recovery"
git fetch https://github.com/kumajaya/android_build
git cherry-pick 36decbfa3fd048a6b27e57f2259f2a1aef129e4c
echo "----------------------------------------"
cd ..
echo "Done!"
