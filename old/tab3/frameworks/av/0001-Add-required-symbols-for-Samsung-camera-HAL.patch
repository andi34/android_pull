From 6efb57fcdafb139559ab93b751fcd332ca296a3b Mon Sep 17 00:00:00 2001
From: Ketut Putu Kumajaya <ketut.kumajaya@gmail.com>
Date: Wed, 23 Apr 2014 12:40:21 +0700
Subject: [PATCH] camera: Add required symbols for Samsung camera HAL

For lt01 Tab 3 8.0 and kona Note 8.0 family

Change-Id: I1dd7be2a14c50e976a7033d6cc4a6222950ce962
---
 camera/CameraParameters.cpp       | 13 +++++++++++++
 include/camera/CameraParameters.h | 13 +++++++++++++
 2 files changed, 26 insertions(+)

diff --git a/camera/CameraParameters.cpp b/camera/CameraParameters.cpp
index 09e0e935982..c10447cbe79 100644
--- a/camera/CameraParameters.cpp
+++ b/camera/CameraParameters.cpp
@@ -162,6 +162,19 @@ const char CameraParameters::KEY_METERING[] = "metering";
 const char CameraParameters::KEY_WDR[] = "wdr";
 const char CameraParameters::KEY_WEATHER[] = "weather";
 const char CameraParameters::KEY_CITYID[] = "contextualtag-cityid";
+
+// For lt01 Tab 3 8.0 and kona Note 8.0 camera HAL
+#ifndef QCOM_HARDWARE
+const char CameraParameters::KEY_ISO[] = "iso";
+const char CameraParameters::METERING_CENTER[] = "center";
+const char CameraParameters::METERING_SPOT[] = "spot";
+const char CameraParameters::METERING_MATRIX[] = "matrix";
+const char CameraParameters::ISO_AUTO[] = "auto";
+const char CameraParameters::ISO_50[] = "ISO50";
+const char CameraParameters::ISO_100[] = "ISO100";
+const char CameraParameters::ISO_200[] = "ISO200";
+const char CameraParameters::ISO_400[] = "ISO400";
+#endif
 #endif
 
 #ifdef HTC_CAMERA_HARDWARE
diff --git a/include/camera/CameraParameters.h b/include/camera/CameraParameters.h
index 4a5ec8fbf4e..42f8b272b0d 100644
--- a/include/camera/CameraParameters.h
+++ b/include/camera/CameraParameters.h
@@ -646,6 +646,19 @@ class CameraParameters
     static const char KEY_WDR[];
     static const char KEY_WEATHER[];
     static const char KEY_CITYID[];
+
+// For lt01 Tab 3 8.0 and kona Note 8.0 camera HAL
+#ifndef QCOM_HARDWARE
+    static const char KEY_ISO[];
+    static const char METERING_CENTER[];
+    static const char METERING_SPOT[];
+    static const char METERING_MATRIX[];
+    static const char ISO_AUTO[];
+    static const char ISO_50[];
+    static const char ISO_100[];
+    static const char ISO_200[];
+    static const char ISO_400[];
+#endif
 #endif
 
 #ifdef HTC_CAMERA_HARDWARE
